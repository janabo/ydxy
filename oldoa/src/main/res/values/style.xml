<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="setting_userinfo_tip">
        <item name="android:textColor">#313131</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">15.0dip</item>
    </style>

    <!--<style name="setting_txt">-->
        <!--<item name="android:textColor">#212121</item>-->
        <!--<item name="android:textSize">14sp</item>-->
    <!--</style>-->

    <style name="menulist">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:dividerHeight">0px</item>
        <item name="android:listSelector">@android:color/transparent</item>
        <item name="android:divider">@null</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="setting_userinfo_txt">
        <item name="android:textColor">#626262</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginLeft">10dip</item>
    </style>

    <style name="setting_userinfo_title">
        <item name="android:width">60dip</item>
        <item name="android:textColor">#a5a5a5</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginRight">10dip</item>
    </style>

    <style name="wheel_title">
        <item name="android:visibility">gone</item>
        <item name="android:textColor">#626262</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingBottom">5.0dip</item>
    </style>

    <style name="marquee">
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">marquee</item>
        <item name="android:focusable">true</item>
        <item name="android:focusableInTouchMode">true</item>
        <item name="android:scrollHorizontally">true</item>
        <item name="android:marqueeRepeatLimit">marquee_forever</item>
    </style>

    <style name="title_txt">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="but_dialog">
        <item name="android:textColor">#515151</item>
        <item name="android:textSize">16dip</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/dialog_but</item>
    </style>

    <style name="but_back">
        <item name="android:paddingLeft">8dip</item>
        <item name="android:text"> 返回</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">16sp</item>
        <item name="android:singleLine">true</item>
        <item name="android:background">@drawable/button_back1</item>
        <item name="android:gravity">center_vertical|center_horizontal</item>
    </style>

    <style name="mylist">
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:dividerHeight">1px</item>
        <item name="android:divider">@drawable/list_divider</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:fadingEdge">none</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="translucentDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="txt">
        <item name="android:textSize">16sp</item>
        <item name="android:autoLink">all</item>
        <item name="android:textColor">#535353</item>
    </style>

    <style name="txt_button">
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">10dip</item>
        <item name="android:height">45dip</item>
    </style>

    <style name="cancel_button">
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/button_cancel</item>
        <item name="android:textColor">#313131</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">10dip</item>
        <item name="android:height">45dip</item>
    </style>

    <style name="ok_button">
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">10dip</item>
        <item name="android:height">45dip</item>
    </style>

    <style name="edittext_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">10dip</item>
        <item name="android:paddingTop">5dip</item>
        <item name="android:textColor">#898989</item>
        <item name="android:paddingBottom">5dip</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="txt_tip">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#898989</item>
    </style>

    <style name="AnimBottom" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/push_bottom_out</item>
    </style>

    <style name="MyDialogStyleBottom" parent="android:Theme.Dialog">
        <item name="android:windowAnimationStyle">@style/AnimBottom</item>
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item>
        <!-- 模糊 -->
    </style>

    <style name="ProgressBar" parent="@android:style/Widget.ProgressBar">
        <item name="android:indeterminateOnly">true</item>
        <item name="android:indeterminateDrawable">@drawable/cycle_anim</item>
    </style>

    <style name="progressDialog" parent="android:style/Theme.Translucent">
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>


    <style name="login_title_txt">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="pickTheme">
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:textColor">#1298FF</item>
        <item name="android:layout_marginLeft">10dip</item>
        <item name="android:layout_marginRight">10dip</item>
    </style>

    <style name="oa_content">
        <item name="android:textColor">#cfcfcf</item>
