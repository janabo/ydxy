<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppThemeWsjc" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#6F7075</item>
        <item name="colorPrimaryDark">#6F7075</item>
        <item name="colorAccent">#6F7075</item>
    </style>

    <style name="AppThemeWsjc.NoTitle" parent="AppThemeWsjc">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="detail_title">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#212121</item>
        <item name="android:textSize">?attr/textSize15</item>
        <item name="android:paddingLeft">10dip</item>
    </style>

    <style name="detail_value">
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#212121</item>
        <item name="android:textSize">?attr/textSize15</item>
        <item name="android:paddingLeft">20dip</item>
        <item name="android:paddingRight">10dip</item>
    </style>

</resources>
